{
  "presets": [
    "@babel/preset-react",
    ["@babel/preset-env", {
      "modules": "auto",
      "useBuiltIns": "usage",
      "corejs": "3.9"
    }]
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties"
  ],
  "env": {
    "cj": {
      "presets": [
        "@babel/preset-react",
        ["@babel/preset-env", {
          "modules": "commonjs",
          "useBuiltIns": "usage",
          "corejs": "3.9"
        }]],
      "plugins": [
        "@babel/plugin-proposal-class-properties"
      ]
    }
  }
}
