{
  "presets": [
    "@babel/preset-react",
    ["@babel/preset-env", {
      "modules": false,
      "useBuiltIns": "usage",
      "corejs": "core-js@3"
    }]
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties"
  ],
  "env": {
    "cj": {
      "presets": [
        "@babel/preset-react",
        ["@babel/preset-env", {
          "modules": "commonjs",
          "useBuiltIns": "usage",
          "corejs": "core-js@3"
        }]],
      "plugins": [
        "@babel/plugin-proposal-class-properties"
      ]
    }
  }
}
