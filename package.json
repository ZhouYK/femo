{
  "description": "frontend data management",
  "version": "1.14.0-alpha.3",
  "keywords": [
    "functional program",
    "frontend",
    "data management",
    "redux"
  ],
  "repository": "https://github.com/ZhouYK/femo.git",
  "author": "zhouyk",
  "license": "MIT",
  "main": "dist/cj/index.js",
  "name": "femo",
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "verbose": true,
    "transform": {
      "^.+\\.(ts|js)x?$": "ts-jest"
    },
    "preset": "ts-jest/presets/js-with-ts",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.ts",
      "\\.(css|less)$": "<rootDir>/test/__mocks__/styleMock.ts"
    },
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx",
      "node",
      "d.ts"
    ],
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "globals": {
      "ts-jest": {
        "tsconfig": "<rootDir>/test/tsconfig.fortest.json"
      }
    }
  },
  "browserslist": [
    "> 1%",
    "last 10 version",
    "not dead",
    "ie >= 9"
  ],
  "scripts": {
    "codecov": "codecov -t 49032a93-7d9a-4205-b14f-2ce81b8a8662",
    "clean-jsx": "rm -f src/*.jsx && rm -f src/**/*.jsx && rm -f src/**/**/*.jsx",
    "clean-js": "rm -f src/*.js && rm -f src/**/*.js && rm -f src/**/**/*.js",
    "clean": "npm run clean-js && npm run clean-jsx",
    "build": "npm run test && npm run tsc && npm run cj && npm run es && npm run umd && npm run clean",
    "tsc": "tsc -p ./",
    "cj": "cross-env NODE_ENV=cj babel src --out-dir dist/cj",
    "es": "babel src --out-dir dist/es",
    "start": "webpack serve --config webpack.dev.config.js",
    "test": "npm run clean && jest --no-cache",
    "umd": "webpack --config webpack.config.js"
  },
  "devDependencies": {
    "@babel/cli": "~7.13.10",
    "@babel/core": "~7.13.10",
    "@babel/plugin-proposal-class-properties": "~7.13.0",
    "@babel/preset-env": "~7.13.10",
    "@babel/preset-react": "~7.12.13",
    "@testing-library/react-hooks": "~7.0.1",
    "@types/jest": "~24.0.25",
    "@types/react": "~17.0.14",
    "@types/react-dom": "~17.0.9",
    "@typescript-eslint/eslint-plugin": "~4.28.4",
    "@typescript-eslint/parser": "~4.22.0",
    "babel-loader": "~8.1.0",
    "codecov": "~3.7.1",
    "cross-env": "~5.2.1",
    "eslint": "~7.31.0",
    "eslint-config-airbnb-base": "~14.2.1",
    "eslint-config-prettier": "~6.11.0",
    "eslint-import-resolver-alias": "~1.1.2",
    "eslint-loader": "~4.0.2",
    "eslint-plugin-import": "~2.23.4",
    "eslint-plugin-jest": "~24.4.0",
    "eslint-plugin-react": "~7.24.0",
    "eslint-plugin-react-hooks": "~4.0.0",
    "file-loader": "~6.0.0",
    "html-webpack-plugin": "~5.2.0",
    "jest": "~27.0.6",
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "regenerator-runtime": "~0.13.9",
    "ts-jest": "~27.0.3",
    "ts-loader": "~9.2.3",
    "typescript": "~4.3.5",
    "url-loader": "~2.1.0",
    "webpack": "~5.24.2",
    "webpack-cli": "~4.5.0",
    "webpack-dev-server": "~3.11.2"
  },
  "files": [
    "src",
    "dist/es",
    "dist/cj",
    "dist/umd",
    "package.json",
    "index.d.ts"
  ],
  "dependencies": {
    "core-js": "~3.9.1"
  }
}
