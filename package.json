{
  "description": "frontend data management",
  "version": "3.0.5",
  "keywords": [
    "functional program",
    "frontend",
    "data management",
    "redux"
  ],
  "repository": "https://github.com/ZhouYK/femo.git",
  "author": "zhouyk",
  "license": "MIT",
  "main": "dist/cj/index.js",
  "module": "dist/es/index.js",
  "name": "femo",
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "verbose": true,
    "testTimeout": 10000,
    "transform": {
      "^.+\\.(ts|js)x?$": "ts-jest"
    },
    "preset": "ts-jest/presets/js-with-ts",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.ts",
      "\\.(css|less)$": "<rootDir>/test/__mocks__/styleMock.ts"
    },
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx",
      "node",
      "d.ts"
    ],
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "globals": {
      "ts-jest": {
        "useESM": true,
        "diagnostics": {
          "warnOnly": true
        }
      }
    }
  },
  "browserslist": [
    "> 1%",
    "last 10 version",
    "not dead",
    "ie >= 9"
  ],
  "scripts": {
    "codecov": "codecov -t 49032a93-7d9a-4205-b14f-2ce81b8a8662",
    "clean": "rm -rf dist",
    "build:cj": "cross-env NODE_ENV=cj babel --extensions \".ts,.tsx\" --presets @babel/preset-typescript src --out-dir dist/cj",
    "build:es": "babel --extensions \".ts,.tsx\" --presets @babel/preset-typescript src --out-dir dist/es",
    "prebuild": "concurrently \"npm run clean\" \"npm run test\"",
    "build": "concurrently -m 6 'npm:build:*'",
    "test": "jest --no-cache --maxWorkers 2",
    "build:umd": "rollup --config rollup.config.js --environment NODE_ENV:production"
  },
  "devDependencies": {
    "@babel/cli": "~7.13.10",
    "@babel/core": "~7.13.10",
    "@babel/preset-env": "~7.13.10",
    "@babel/preset-react": "~7.16.7",
    "@babel/preset-typescript": "~7.16.7",
    "@rollup/plugin-babel": "~5.3.0",
    "@rollup/plugin-commonjs": "~21.0.1",
    "@rollup/plugin-node-resolve": "~13.1.3",
    "@rollup/plugin-typescript": "~8.3.0",
    "@testing-library/jest-dom": "~5.16.4",
    "@testing-library/react": "~12.1.5",
    "@testing-library/react-hooks": "~7.0.1",
    "@types/jest": "~24.0.25",
    "@types/react": "~17.0.14",
    "@types/react-dom": "~17.0.9",
    "@typescript-eslint/eslint-plugin": "~4.28.4",
    "@typescript-eslint/parser": "~4.22.0",
    "codecov": "~3.7.1",
    "concurrently": "~7.0.0",
    "cross-env": "~5.2.1",
    "eslint": "~7.31.0",
    "eslint-config-airbnb-base": "~14.2.1",
    "eslint-config-prettier": "~6.11.0",
    "eslint-import-resolver-alias": "~1.1.2",
    "eslint-plugin-import": "~2.23.4",
    "eslint-plugin-jest": "~24.4.0",
    "eslint-plugin-react": "~7.24.0",
    "eslint-plugin-react-hooks": "~4.0.0",
    "jest": "~27.0.6",
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "regenerator-runtime": "~0.13.9",
    "rollup": "~2.67.3",
    "rollup-plugin-terser": "~7.0.2",
    "ts-jest": "~27.0.3",
    "tslib": "~2.4.0",
    "typescript": "~4.3.5"
  },
  "files": [
    "src",
    "dist/es",
    "dist/cj",
    "dist/umd",
    "package.json",
    "index.d.ts"
  ],
  "peerDependencies": {
    "react": ">=16.8.0"
  }
}
