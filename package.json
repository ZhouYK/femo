{
  "description": "frontend data management",
  "version": "0.3.1",
  "keywords": [
    "functional program",
    "frontend",
    "data management",
    "redux"
  ],
  "repository": "https://github.com/ZhouYK/femo.git",
  "author": "zhouyk",
  "license": "MIT",
  "main": "dist/es/index.js",
  "name": "femo",
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "verbose": true,
    "transform": {
      "^.+\\.(ts|js)x?$": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.ts",
      "\\.(css|less)$": "<rootDir>/test/__mocks__/styleMock.ts"
    },
    "moduleDirectories": [
      "node_modules"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/test/tsconfig.fortest.json"
      }
    }
  },
  "browserslist": [
    "> 1%",
    "last 10 version",
    "not dead",
    "ie >= 9"
  ],
  "scripts": {
    "codecov": "codecov -t 49032a93-7d9a-4205-b14f-2ce81b8a8662",
    "clean-js": "rm -f src/*.js",
    "build": "npm run test && npm run tsc && npm run cj && npm run es && npm run umd && npm run clean-js",
    "tsc": "tsc -p ./",
    "cj": "cross-env NODE_ENV=cj babel src --out-dir dist/cj",
    "es": "babel src --out-dir dist/es",
    "start": "webpack-dev-server --config webpack.dev.config.js",
    "test": "npm run clean-js && jest --no-cache",
    "umd": "webpack --config webpack.config.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "@types/jest": "~24.0.13",
    "@types/react": "~16.8.17",
    "@types/react-dom": "~16.8.4",
    "@typescript-eslint/eslint-plugin": "~1.9.0",
    "@typescript-eslint/parser": "~1.9.0",
    "babel-loader": "^8.0.5",
    "codecov": "~3.5.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "femo-react-helper": "0.0.6",
    "file-loader": "~4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.5.0",
    "react": "~16.8.6",
    "react-dom": "~16.8.6",
    "ts-jest": "^24.0.0",
    "ts-loader": "^5.3.3",
    "typescript": "^3.3.3333",
    "url-loader": "~2.1.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1"
  },
  "files": [
    "src",
    "dist/es",
    "dist/cj",
    "dist/umd",
    "package.json",
    "index.d.ts"
  ],
  "dependencies": {
    "@babel/polyfill": "~7.4.4",
    "core-js": "~3.0.1"
  }
}
